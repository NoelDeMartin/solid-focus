{
    "name": "solid-focus",
    "version": "0.3.0",
    "private": true,
    "type": "module",
    "repository": "github:noeldemartin/solid-focus",
    "scripts": {
        "build": "vite build",
        "cy:dev": "concurrently --kill-others \"npm run test:serve-app\" \"npm run test:serve-pod\" \"npm run cy:open\"",
        "cy:open": "cypress open --config-file ./cypress/cypress.config.ts --e2e --browser chromium",
        "cy:run": "cypress run --config-file ./cypress/cypress.config.ts",
        "cy:test": "start-server-and-test test:serve-app http-get://localhost:5001 test:serve-pod http-get://localhost:3000 cy:run",
        "cy:test-snapshots": "docker run -it -u `id -u ${whoami}` -e CYPRESS_SNAPSHOTS=true -v ./:/app -w /app cypress/base:18.16.0 sh -c \"npx cypress install && npm run cy:test\"",
        "cy:test-snapshots:ci": "docker run -e CYPRESS_SNAPSHOTS=true -v ./:/app -w /app cypress/base:18.16.0 sh -c \"npx cypress install && npm run cy:test\"",
        "dev": "vite",
        "dev:serve-pod": "community-solid-server -c @css:config/file.json -f ./solid",
        "lint": "noeldemartin-lint src",
        "test": "vitest --run",
        "test:ci": "vitest --run --reporter verbose",
        "test:serve-app": "vite --port 5001 --mode testing",
        "test:serve-pod": "community-solid-server -l warn"
    },
    "dependencies": {
        "@aerogel/core": "0.0.0-next.0dddfa5a33f1886a059ceb0950867ddcfa86480d",
        "@aerogel/plugin-i18n": "0.0.0-next.0dddfa5a33f1886a059ceb0950867ddcfa86480d",
        "@aerogel/plugin-local-first": "0.0.0-next.adad855f8b03c06c6a43b72de3b00a26e6f700a1",
        "@aerogel/plugin-routing": "0.0.0-next.0dddfa5a33f1886a059ceb0950867ddcfa86480d",
        "@aerogel/plugin-solid": "0.0.1-next.0dddfa5a33f1886a059ceb0950867ddcfa86480d",
        "@aerogel/plugin-soukai": "0.0.0-next.0dddfa5a33f1886a059ceb0950867ddcfa86480d",
        "@headlessui/vue": "^1.7.22",
        "@intlify/unplugin-vue-i18n": "^0.12.2",
        "@noeldemartin/solid-utils": "^0.6.0",
        "@noeldemartin/utils": "^0.7.0",
        "@popperjs/core": "^2.11.8",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "popmotion": "^11.0.5",
        "soukai": "^0.6.0",
        "soukai-solid": "^0.6.0",
        "tailwind-merge": "^2.3.0",
        "tailwindcss": "^3.3.2",
        "vivant": "^0.0.1",
        "vue": "^3.3.0",
        "vue-i18n": "9.3.0-beta.19"
    },
    "devDependencies": {
        "@aerogel/cli": "0.0.0-next.0dddfa5a33f1886a059ceb0950867ddcfa86480d",
        "@aerogel/cypress": "0.0.0-next.0dddfa5a33f1886a059ceb0950867ddcfa86480d",
        "@aerogel/vite": "0.0.0-next.8ba256f27dca550f69d817d015f40e2d3dc9977f",
        "@iconify/json": "^2.2.134",
        "@noeldemartin/eslint-config-vue": "0.1.1-next.8da46d7532acbe6ee4126f410141b7f281ec349a",
        "@noeldemartin/scripts": "0.3.0-next.88fb0ff21ba1f9015dab043ded20be9518e51248",
        "@solid/community-server": "^7.1.6",
        "@total-typescript/ts-reset": "^0.4.2",
        "@tsconfig/node22": "^22.0.1",
        "@types/node": "^20.3.1",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.2.0",
        "cypress": "^13.17.0",
        "postcss-pseudo-classes": "^0.4.0",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "start-server-and-test": "^2.0.0",
        "typescript": "^5.8.2",
        "unplugin-icons": "^0.16.3",
        "unplugin-vue-components": "^0.24.1",
        "vite": "^6.2.2",
        "vitest": "^3.0.9",
        "vue-tsc": "^2.2.8"
    },
    "eslintConfig": {
        "extends": [
            "@noeldemartin/eslint-config-vue"
        ]
    },
    "prettier": {
        "plugins": [
            "prettier-plugin-tailwindcss"
        ],
        "printWidth": 120
    }
}
