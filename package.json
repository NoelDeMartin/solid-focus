{
    "name": "solid-focus",
    "version": "0.3.0",
    "private": true,
    "repository": "github:noeldemartin/solid-focus",
    "scripts": {
        "build": "vite build",
        "cy:dev": "concurrently --kill-others \"npm run test:serve-app\" \"npm run test:serve-pod\" \"npm run cy:open\"",
        "cy:open": "cypress open --config-file ./cypress/cypress.config.ts --e2e --browser chromium",
        "cy:run": "cypress run --config-file ./cypress/cypress.config.ts",
        "cy:test": "start-server-and-test test:serve-app http-get://localhost:5001 test:serve-pod http-get://localhost:3000 cy:run",
        "cy:test-snapshots": "docker run -it -u `id -u ${whoami}` -e CYPRESS_SNAPSHOTS=true -v ./:/app -w /app cypress/base:18.16.0 sh -c \"npx cypress install && npm run cy:test\"",
        "cy:test-snapshots:ci": "docker run -e CYPRESS_SNAPSHOTS=true -v ./:/app -w /app cypress/base:18.16.0 sh -c \"npx cypress install && npm run cy:test\"",
        "dev": "vite",
        "dev:serve-pod": "community-solid-server -c @css:config/file.json -f ./solid",
        "histoire": "histoire dev",
        "lint": "noeldemartin-lint src",
        "postinstall": "patch-package",
        "test": "vitest --run",
        "test:ci": "vitest --run --reporter verbose",
        "test:serve-app": "vite --port 5001 --mode testing",
        "test:serve-pod": "community-solid-server -l warn"
    },
    "dependencies": {
        "@aerogel/core": "0.0.0-next.906ec80f260b7e5cb54a0c97bd4905bdaf4bf916",
        "@aerogel/plugin-i18n": "0.0.0-next.5db4f0ca00e272b7c47941039d7b104c51c246b0",
        "@aerogel/plugin-offline-first": "0.0.0-next.b1bbfb972b88fe7cc5caa9c0005d4d82e25e9ae3",
        "@aerogel/plugin-routing": "0.0.0-next.9f3ed4cb5fa9fb0b4794b0def141e8d82563bd40",
        "@aerogel/plugin-solid": "0.0.1-next.7922f2bc4a4504bc48c8991b91c146ff6ac8a093",
        "@aerogel/plugin-soukai": "0.0.0-next.576be48ca5479b8b603fa3960d1b6eb3b5d90a3c",
        "@headlessui/vue": "^1.7.22",
        "@intlify/unplugin-vue-i18n": "^0.12.2",
        "@noeldemartin/solid-utils": "0.5.0-next.612e9e93903ec33bbcba7105ed2a8580ed2d754c",
        "@noeldemartin/utils": "0.6.0-next.036a147180df61c600c4599df30816ac860dbf06",
        "@popperjs/core": "^2.11.8",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "popmotion": "^11.0.5",
        "soukai": "0.5.2-next.b7fedddf243ea6df3baf082890eb2834dd48c010",
        "soukai-solid": "0.5.2-next.6311ad111723b5591e1f5877af08e4caf68280d8",
        "tailwind-merge": "^2.3.0",
        "tailwindcss": "^3.3.2",
        "vivant": "^0.0.1",
        "vue": "^3.3.0",
        "vue-i18n": "9.3.0-beta.19"
    },
    "devDependencies": {
        "@aerogel/cli": "0.0.0-next.13b88f634a31caf3669d61e53e7d3fa95e20488c",
        "@aerogel/cypress": "0.0.0-next.ecf4c2737b43d20cfac283ce80ac5913de65f502",
        "@aerogel/histoire": "0.0.0-next.13b88f634a31caf3669d61e53e7d3fa95e20488c",
        "@aerogel/vite": "0.0.0-next.ad39a2a31f43d07c0518ac80d8b1a8e2f823027d",
        "@iconify/json": "^2.2.134",
        "@noeldemartin/eslint-config-vue": "next",
        "@noeldemartin/scripts": "next",
        "@solid/community-server": "^7.1.6",
        "@total-typescript/ts-reset": "^0.4.2",
        "@types/node": "^20.3.1",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.2.0",
        "cypress": "^12.17.0",
        "eslint": "^8.40.0",
        "histoire": "0.17.6",
        "patch-package": "^8.0.0",
        "postcss-pseudo-classes": "^0.4.0",
        "prettier": "^2.8.8",
        "prettier-eslint-cli": "^7.1.0",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "start-server-and-test": "^2.0.0",
        "typescript": "~5.4",
        "unplugin-icons": "^0.16.3",
        "unplugin-vue-components": "^0.24.1",
        "vite": "^4.3.0",
        "vitest": "^0.33.0",
        "vue-tsc": "^1.8.15"
    },
    "overrides": {
        "@noeldemartin/solid-utils": "0.5.0-next.612e9e93903ec33bbcba7105ed2a8580ed2d754c"
    },
    "eslintConfig": {
        "extends": [
            "@noeldemartin/eslint-config-vue"
        ]
    },
    "prettier": {
        "plugins": [
            "prettier-plugin-tailwindcss"
        ],
        "printWidth": 120
    }
}
